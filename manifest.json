{
  "manifest_version": 2,

  "name": "leetcode-ignore",
  "version": "1.0.0",
  "description": "",
  "browser_action": {
    "default_popup": "leetcode.html"   
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "run_at": "document_end",
      "css": ["leetcode.css"],
      "js": ["jquery.min.js", "leetcode.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "background",
    "tabs"
  ],
  "web_accessible_resources": [
  ]
}
